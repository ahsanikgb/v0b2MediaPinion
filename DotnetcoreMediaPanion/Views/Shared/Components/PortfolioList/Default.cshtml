@model IEnumerable<DotnetcoreMediaPanion.ViewModels1.Portfolio>
@{ 
    Layout = null;
}

<div class="container">
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
            <div class="section-headline text-center">
                <h2>Our Portfolio</h2>
            </div>
        </div>
    </div>
  
    <div class="row">
        <!-- Start Portfolio -page -->
        <div class="awesome-project-1 fix">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="awesome-menu ">
                    <ul class="project-menu">
                        <li>
                            <a href="#" class="active" data-filter="*">All</a>
                        </li>
                        <li>
                            <a href="#" data-filter=".vedio">Vedio</a>
                        </li>
                        <li>
                            <a href="#" data-filter=".audio">Audio</a>
                        </li>
                        <li>
                            <a href="#" data-filter=".designing">Designing</a>
                        </li>
                        <li>
                            <a href="#" data-filter=".graphics">Graphics</a>
                        </li>
                        <li>
                            <a href="#" data-filter=".media">Media Placement</a>
                        </li>
                        <li>
                            <a href="#" data-filter=".Print">Print</a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="awesome-project-content">
            <!-- single-awesome-project start -->
            @foreach (var item in Model)
            {




                if (item.Name == "vedio")
                {
                   
                    <div class="col-md-4 col-sm-4 col-xs-12 @item.Name">
                        <div class="single-awesome-project">
                            <div class="awesome-img">
                                <a href="#">
                                    <a href="#"><img src="~/web/img/portfolio/@item.Image" target="_blank" alt="" /></a>
                                </a>
                                <div class="add-actions text-center">
                                    <div class="project-dec">
                                        <a class="" data-gall="" href="@item.Src" target="_blank">

                                            <h4>@item.Heading</h4>
                                            <span>@item.SubHeading</span>
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    }
                    else
                    {
                    <div class="col-md-4 col-sm-4 col-xs-12 @item.Name">
                        <div class="single-awesome-project">
                            <div class="awesome-img">
                                <a href="#">
                                    <a href="#"><img src="~/web/img/portfolio/@item.Image" target="_blank" alt="" /></a>
                                </a>
                                <div class="add-actions text-center">
                                    <div class="project-dec">
                                        <a class="venobox" data-gall="myGallery" href="~/web/img/portfolio/@item.Image" target="_blank">

                                            <h4>@item.Heading</h4>
                                            <span>@item.SubHeading</span>
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>



                    }




                
            }
            <!-- single-awesome-project end -->
        </div>

    </div>
